<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                      http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
  version="4.0"
  metadata-complete="true">

    <request-character-encoding>UTF-8</request-character-encoding>
	
	<!-- Mapping --> 
    <servlet>
      <servlet-name>MachinesPage</servlet-name>
      <servlet-class>MachinesPageHandler</servlet-class>
      <!--<async-supported>true</async-supported>-->
    </servlet>
    <servlet-mapping>
      <servlet-name>MachinesPage</servlet-name>
      <url-pattern>/MachinesPageHandler</url-pattern>
    </servlet-mapping>
	
	
	 <servlet>
      <servlet-name>LogTablePage</servlet-name>
      <servlet-class>LogTableHandler</servlet-class>
      <!--<async-supported>true</async-supported>-->
    </servlet>
    <servlet-mapping>
      <servlet-name>LogTablePage</servlet-name>
      <url-pattern>/LogTablePageHandler</url-pattern>
    </servlet-mapping>
	
	
	<servlet>
      <servlet-name>AddMachinePage</servlet-name>
      <servlet-class>AddMachinePageHandler</servlet-class>
      <!--<async-supported>true</async-supported>-->
    </servlet>
    <servlet-mapping>
      <servlet-name>AddMachinePage</servlet-name>
      <url-pattern>/AddMachinePageHandler</url-pattern>
    </servlet-mapping>
	
	<servlet>
      <servlet-name>LeaderBoardPage</servlet-name>
      <servlet-class>LeaderBoardPageHandler</servlet-class>
      <!--<async-supported>true</async-supported>-->
    </servlet>
    <servlet-mapping>
      <servlet-name>LeaderBoardPage</servlet-name>
      <url-pattern>/LeaderBoardPageHandler</url-pattern>
    </servlet-mapping>
	
	<servlet>
      <servlet-name>RemoveMachine</servlet-name>
      <servlet-class>RemoveMachineHandler</servlet-class>
      <!--<async-supported>true</async-supported>-->
    </servlet>
    <servlet-mapping>
      <servlet-name>RemoveMachine</servlet-name>
      <url-pattern>/RemoveMachineHandler</url-pattern>
    </servlet-mapping>
	
	

	<!-- welcome -->
    <welcome-file-list>
        <!--<welcome-file>HomePage.html</welcome-file>-->
		<welcome-file>HomePage.html</welcome-file>
    </welcome-file-list>
</web-app>
